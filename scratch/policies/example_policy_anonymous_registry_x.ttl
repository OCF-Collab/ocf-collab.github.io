<https://t3innovationnetwork.org/networktrust/policies/29905411-553e-4872-9baf-e401dac157d2> a odrl:Agreement ;
    odrl:profile <https://t3innovationnetwork.org/networktrust/profiles/d724c6b2-16e6-4cc2-9f19-0524d102474f> ;
    dc:creator "Competencies Anonymous Registry" ;
    dc:description "This policy covers all permitted and prohibited Network node actions on competency frameworks in the Competencies Anonymous Registry." ;
    dc:issued "2020-02-20T08:15:30-05:00" ;
    odrl:assignee <https://t3innovationnetwork.org/networktrust/agents/12c906b1-8243-4bac-a5d8-b044ffdfc893> ;
    odrl:assigner <https://t3innovationnetwork.org/networktrust/agents/d9eb10b4-fc92-4d82-8e6e-c4c054ad70b8> ;
    odrl:permission [ a odrl:Permission ;
            odrl:action odrl:index ;
            odrl:target <https://t3innovationnetwork.org/networktrust/assetCollection/01bca768-c435-4237-9e42-a3df0965334e> ],
        [ a odrl:Permission ;
            odrl:action odrl:derive ;
            odrl:target <https://t3innovationnetwork.org/networktrust/assetCollection/01bca768-c435-4237-9e42-a3df0965334e> ],
        [ a odrl:Permission ;
            odrl:action odrl:transform ;
