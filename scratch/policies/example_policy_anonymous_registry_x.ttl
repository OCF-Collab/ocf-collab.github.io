            odrl:action odrl:display ;
            odrl:target <https://t3innovationnetwork.org/networktrust/assetCollection/01bca768-c435-4237-9e42-a3df0965334e> ;
            odrl:constraint [ a odrl:Constraint ;
                    odrl:leftOperand odrl:elapsedTime ;
                    odrl:operator odrl:lt ;
                    odrl:rightOperand "P48H"^^xsd:duration 
            ] ;
            odrl:duty [ a cc:Attribution ;
                    odrl:action odrl:attribute
                    cc:attributionName "Competencies Anonymous Registry" ;
                    cc:attributionURL "http://anonymousCompetencies.org" 
            ] ; 
        ] ;
    odrl:prohibition [ a odrl:Prohibition ;
            odrl:action odrl:distribute ;
            odrl:target <https://t3innovationnetwork.org/networktrust/assetCollection/01bca768-c435-4237-9e42-a3df0965334e> 
